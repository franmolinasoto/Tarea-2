        "std_obs1 = obs_1.std()\n",
        "limite_sup = valor_medio_ruido + 3*std_obs1\n",
        "limite_inf = valor_medio_ruido - 3*std_obs1\n",
        "ruido = obs_1[(obs_1 > limite_inf) & (obs_1 < limite_sup)]\n",
        "ruido\n"
